{
  "crons": [
    {
      "path": "/api/cron/sync-blogs",
      "schedule": "0 2 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/service",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/service/:slug",
      "destination": "/services/:slug",
      "permanent": true
    },
    {
      "source": "/location",
      "destination": "/locations",
      "permanent": true
    },
    {
      "source": "/location/:city",
      "destination": "/locations/:city",
      "permanent": true
    },
    {
      "source": "/about-us",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/aboutus",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/contact-us",
      "destination": "/contact",
      "permanent": true
    },
